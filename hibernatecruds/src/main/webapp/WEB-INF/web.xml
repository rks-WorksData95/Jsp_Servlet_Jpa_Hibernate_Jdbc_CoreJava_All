<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>insertStudent</servlet-name>
  	<display-name>insertStudent</display-name>
  	<description></description>
  	<servlet-class>com.controller.insertStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditAndDeleteStudent</servlet-name>
  	<display-name>EditAndDeleteStudent</display-name>
  	<description></description>
  	<servlet-class>com.controller.EditAndDeleteStudent</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>insertStudent</servlet-name>
  	<url-pattern>/insertStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditAndDeleteStudent</servlet-name>
  	<url-pattern>/EditAndDeleteStudent</url-pattern>
  </servlet-mapping>
</web-app>
